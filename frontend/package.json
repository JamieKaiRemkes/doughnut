{
  "name": "doughnut-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9.12.3"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "syncpack fix-mismatches",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome check --write ."
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "5.3.3",
    "file-saver": "^2.0.5",
    "gsap": "^3.12.5",
    "intersection-observer": "^0.12.2",
    "marked": "^14.1.3",
    "mini-debounce": "^1.0.8",
    "quill": "2.0.2",
    "turndown": "^7.2.0",
    "vue": "3.5.12",
    "vue-content-loader": "2.0.1",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/turndown": "^5.0.5",
    "@vitejs/plugin-vue": "^5.1.4",
