{
  "name": "doughnut-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=23",
    "pnpm": ">=9.15.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "syncpack fix-mismatches",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome check --write ."
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "gsap": "^3.12.5",
    "intersection-observer": "^0.12.2",
    "marked": "^15.0.4",
    "mini-debounce": "^1.0.8",
    "quill": "2.0.3",
    "turndown": "^7.2.0",
    "vue": "3.5.13",
    "vue-content-loader": "2.0.1",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@types/turndown": "^5.0.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.22",
    "es-toolkit": "^1.30.1",
    "jsdom": "^25.0.1",
    "lodash": "^4.17.21",
    "optionator": "^0.9.4",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "0.28.0",
    "unplugin-vue-router": "^0.10.9",
    "vite": "6.0.5",
    "vite-plugin-checker": "0.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "2.1.8",
    "vitest-dom": "^0.1.1",
    "vitest-fetch-mock": "^0.4.2",
    "vue-tsc": "2.1.10"
  }
}
