    "style-loader": "^4.0.0",
    "typescript": "5.6.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "5.4.10",
    "vite-plugin-checker": "0.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "2.1.3",
    "vitest-dom": "^0.1.1",
    "vitest-fetch-mock": "^0.3.0",
    "vue-tsc": "2.1.8"
